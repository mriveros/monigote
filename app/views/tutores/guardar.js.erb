<% if @guardado_ok %>

   $("<div class='col-sm-12' id='msg-agregar-tutor' style='color:#000;padding-top:5px;font-size:10px;background:#d6ffd6;border-radius:5px;text-align:center;'>Registrado Exitosamente.</div>").insertBefore("#form-agregar-tutor");
   
  $("#form_buscar_tutores_id").val('<%= @tutor.id %>');
  $("#form_buscar_tutores_nombres").val('');
  $("#form_buscar_tutores_apellidos").val('');
  $("#form_buscar_tutores_direccion").val('');
  $("#form_buscar_tutores_telefono").val('');
  $("#form-buscar-tutores").submit();

  
  setTimeout(function(){ 
    
    $("#<%= params[:window_id]%>").modal('hide');
    swal("Tutor agregado Exitosamente.");

    }, 2000);


<% else %>
	swal("Error al guardar:" + "<%=j(@msg)%>")
    $("<div class='col-sm-12' id='msg-agregar-tutores' style='color:#000;padding-top:5px;font-size:10px;background: #f8dfdf;border-radius:5px;text-align:center;'><%= j(@msg) %></div>").insertBefore("#form-agregar-tutores");

<% end %>
