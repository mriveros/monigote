<% if @eliminado %>

   $("<div class='col-sm-12' id='msg-eliminar-matriculacion' style='color:#000;padding-top:5px;font-size:10px;background:#d6ffd6;border-radius:5px;text-align:center;'>Eliminado exitosamente.</div>").insertBefore("#table-matriculaciones");
   $("#form_buscar_matricualciones_id").val('<%= @matriculacion_elim.id %>');
   $("#form_buscar_matriculaciones_periodo_id").val('');
   $("#form_buscar_matriculaciones_nivel_id").val('');
   $("#form_buscar_matriculaciones_sala_id").val('');
   $("#form_buscar_matriculaciones_estado_matriculacion_id").val('');
   

    setTimeout(function(){ 

    	$("#msg-eliminar-matriculacion").remove(); 
    	$("#form-buscar-matriculaciones").submit(); 
    }, 2000);
   
    	swal("Eliminado Exitosamente.")

<% else %>

  $("<div class='col-sm-12' id='msg-eliminar-matriculacion' style='color:#000;padding-top:5px;font-size:10px;background: #f8dfdf;border-radius:5px;text-align:center;'>Error al eliminar La Matriculación. Intente más tarde.</div>").insertBefore("#table-matriculaciones");
  

  setTimeout(function(){ 

      $("#msg-eliminar-matriculacion").remove(); 
      swal("<%=@msg%>")
    
    }, 2000);

<% end %>
